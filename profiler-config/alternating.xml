<?xml version="1.0" encoding="utf-8"?>

<config>

    <params>
        <domain_mask>0xb</domain_mask>
    </params>

    <sections>
        <section target="cpu">
            <bounds>
                <func name="(anonymous namespace)::do_work"/>
            </bounds>
            <allow_concurrency/>
